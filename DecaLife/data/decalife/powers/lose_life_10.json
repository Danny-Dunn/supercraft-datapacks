{
    "type": "origins:multiple",
    "hidden": true,
    "check_advancement": {
        "type": "origins:action_over_time",
        "interval": 40,
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "origin set @s origins:origin decalife:life_9_origin"
                },
                {
                    "type": "origins:execute_command",
                    "command": "advancement revoke @s only decalifelibrary:check_death"
                }
            ]
        },
        "condition": {
            "type": "origins:advancement",
            "advancement": "decalifelibrary:check_death"
        }
    }
}